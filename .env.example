# ==================== SiliconFlow API Configuration ====================
# SiliconFlow API Endpoint
SILICONFLOW_API_BASE=https://api.siliconflow.cn/v1
SILICONFLOW_API_KEY=your-siliconflow-api-key-here

# Model Selection
# Core Reasoning (PRD/Analysis): DeepSeek-R1-Distill-Qwen-32B or Qwen2.5-72B-Instruct
MODEL_REASONING=deepseek-ai/DeepSeek-R1-Distill-Qwen-32B
# Fast Routing (Intent Classification): Qwen2.5-7B-Instruct
MODEL_ROUTING=Qwen/Qwen2.5-7B-Instruct
# Reranker: bge-reranker-v2-m3
MODEL_RERANKER=BAAI/bge-reranker-v2-m3

# ==================== Neo4j Configuration ====================
NEO4J_URI=neo4j+s://your-instance.databases.neo4j.io
NEO4J_USERNAME=neo4j
NEO4J_PASSWORD=your-neo4j-password

# ==================== Vector Database Configuration ====================
# ChromaDB Path (local)
VECTOR_DB_PATH=Data/Vector

# Embedding Model (local)
EMBEDDING_MODEL=paraphrase-multilingual-MiniLM-L12-v2

# ==================== Application Settings ====================
# Environment
ENVIRONMENT=development

# Logging Level
LOG_LEVEL=INFO
